  keywords = {Adult,ambulatory,Anhedonia,asociality,avolition,Bipolar Disorder,ecological momentary assessment,Ecological Momentary Assessment,Female,Functional Status,Geographic Mapping,Humans,Male,Middle Aged,mobile health (mHealth),Psychometrics,psychosis,Psychotic Disorders,Reproducibility of Results,Schizophrenia,Social Behavior,Volition},
