@article{sliedrechtVarietyAlcoholUse2022b,
  title = {Variety in {{Alcohol Use Disorder Relapse Definitions}}: {{Should}} the {{Term}} ``{{Relapse}}'' {{Be Abandoned}}?},
  shorttitle = {Variety in {{Alcohol Use Disorder Relapse Definitions}}},
  author = {Sliedrecht, Wilco and Roozen, Hendrik and {de Waart}, Ranne and Dom, Geert and Witkiewitz, Katie},
  year = {2022},
  month = mar,
  journal = {Journal of Studies on Alcohol and Drugs},
  volume = {83},
  number = {2},
  pages = {248--259},
  publisher = {Alcohol Research Documentation, Inc.},
  issn = {1937-1888},
  doi = {10.15288/jsad.2022.83.248},
  urldate = {2024-04-13},
  abstract = {Objective: The definition of relapse has been the subject of debate for decades, and a semantic ambiguity highlighted in a 2016 article in the Journal of Studies on Alcohol and Drugs remains. The current article replicates and extends the 2016 findings on alcohol use disorder (AUD) relapse definitions. Method: We conducted a systematic review of 321 articles that examined relapse in patients with AUD, published from 2000 to 2019. Relapse definitions were extracted and a narrative review of definitions was conducted. Results: One hundred and one different definitions of relapse were used in 251 (78\%) of the reviewed articles. In 70 (22\%) of articles, no definition of AUD relapse was provided. Fifty-three articles used diagnostic criteria (i.e., alcohol use after remission of AUD), whereas 99 articles defined relapse as ``any alcohol use'' or ``any use of alcohol/drugs.'' Additional articles defined relapse by alcohol outcomes (e.g., percent drinking days), alcohol-related problems, or hospitalizations (n = 97). Only 12 articles described the time window of abstinence preceding a relapse. We observed relatively no meaningful intercontinental or time-related differences in relapse definitions, although the outcome ``percent heavy drinking days'' was used more frequently in recent studies. Conclusions: A wide variety of relapse definitions were identified. Despite decades of research and discussion, there is still no widely accepted consensus definition of AUD relapse. We propose to shift the focus toward clinical continuous outcomes, course specifiers based on the number of AUD symptoms present, and quality-of-life--related criteria instead of using current dichotomous AUD relapse terminology.}
}

