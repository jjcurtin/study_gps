  keywords = {Critical Care,Health Care Rationing,Health Equity,Healthcare Disparities,Humans,Length of Stay,Machine Learning,Patient Outcome Assessment,Social Justice},
  file = {/Users/punturierice/Zotero/storage/S765D3NT/Rajkomar et al. - 2018 - Ensuring Fairness in Machine Learning to Advance H.pdf}
}

@article{raughGeolocationDigitalPhenotyping2020,
  title = {Geolocation as a {{Digital Phenotyping Measure}} of {{Negative Symptoms}} and {{Functional Outcome}}},
  author = {Raugh, Ian M. and James, Sydney H. and Gonzalez, Cristina M. and Chapman, Hannah C. and Cohen, Alex S. and Kirkpatrick, Brian and Strauss, Gregory P.},
