@article{vaethDrinkingAlcoholUse2017a,
  title = {Drinking, {{Alcohol Use Disorder}}, and {{Treatment Access}} and {{Utilization Among U}}.{{S}}. {{Racial}}/{{Ethnic Groups}}},
  author = {Vaeth, Patrice A. C. and Wang-Schweig, Meme and Caetano, Raul},
